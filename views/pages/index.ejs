<%- include('./../partials/head.ejs') %>
<body>
    <main class='main'>
        <ul class='messages-container'>
            <% messages.forEach((message) => { %>
                <li class='message'>
                    <h2>
                        <%= message.user %>
                    </h2>
                   <p>
                        <%= message.text %>
                   </p>
                   <span>
                        <%= message.added %>
                   </span>
                </li>
            <% }) %>
        </ul>
    </main>
</body>
